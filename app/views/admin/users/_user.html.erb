<tr id="user-<%= user.id %>">
  <td style="text-align: left;" class="text-center" id="name-<%= user.id %>"><%= link_to user.name, "#" %></td>
  <td class="text-center"><%= user.email %></td>
  <td><%= link_to "#{Bookingtour.search_by_user(user.id).size} booked tour", "#" %></td>
  <td class="text-center">
    <%= link_to "", { action: :destroy, id: user.id }, method: :delete, data: { confirm: "Are you sure?" }, class: "glyphicon glyphicon-floppy-remove" %>
  </td>
</tr>
